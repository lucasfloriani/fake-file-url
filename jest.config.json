{
  "collectCoverage": true,
  "coverageDirectory": "coverage",
  "coverageProvider": "v8",
  "coverageThreshold": {
    "global": {
      "branches": 100,
      "functions": 100,
      "lines": 100,
      "statements": 100
    }
  },
  "testEnvironment": "jsdom",
  "preset": "ts-jest",
  "transform": {
    "^.+\\test.(t|j)s$": "ts-jest"
  },
  "testMatch": ["**/?(*.)+(test).ts"],
  "moduleFileExtensions": ["js", "ts"]
}
